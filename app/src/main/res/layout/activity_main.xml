<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".MainActivity">


    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/ImpHintsRecyclerview"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="8dp"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="8dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

            </androidx.recyclerview.widget.RecyclerView>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/myprofile"
                android:layout_width="80dp"
                android:layout_height="77dp"
                android:layout_marginTop="204dp"
                android:background="@drawable/newprofile1"
                android:contentDescription="@string/desc_button"
                app:layout_constraintEnd_toEndOf="@+id/ImpHintsRecyclerview"
                app:layout_constraintHorizontal_bias="0.038"
                app:layout_constraintStart_toStartOf="@+id/ImpHintsRecyclerview"
                app:layout_constraintTop_toTopOf="@+id/ImpHintsRecyclerview" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:text="My Profile"
                android:textColor="#FF000000"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="@+id/myprofile"
                app:layout_constraintHorizontal_bias="0.452"
                app:layout_constraintStart_toStartOf="@+id/myprofile"
                app:layout_constraintTop_toBottomOf="@+id/myprofile" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/mypassbook"
                android:layout_width="80dp"
                android:layout_height="77dp"
                android:layout_marginTop="204dp"
                android:background="@drawable/newpassbook1"
                android:contentDescription="@string/desc_button"
                app:layout_constraintEnd_toEndOf="@+id/ImpHintsRecyclerview"
                app:layout_constraintHorizontal_bias="0.273"
                app:layout_constraintStart_toEndOf="@+id/myprofile"
                app:layout_constraintTop_toTopOf="@+id/ImpHintsRecyclerview" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:text="MyPassbook"
                android:textColor="#FF000000"
                app:layout_constraintEnd_toEndOf="@+id/mypassbook"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@+id/mypassbook"
                app:layout_constraintTop_toBottomOf="@+id/mypassbook" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/fundtransfer"
                android:layout_width="80dp"
                android:layout_height="77dp"
                android:layout_marginTop="204dp"
                android:background="@drawable/fundtransfer2"
                android:contentDescription="@string/desc_button"
                app:layout_constraintEnd_toEndOf="@+id/ImpHintsRecyclerview"
                app:layout_constraintHorizontal_bias="0.792"
                app:layout_constraintStart_toEndOf="@+id/mypassbook"
                app:layout_constraintTop_toTopOf="@+id/ImpHintsRecyclerview" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:text="Fund Transfer"
                android:textColor="#FF000000"
                app:layout_constraintEnd_toEndOf="@+id/fundtransfer"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/fundtransfer"
                app:layout_constraintTop_toBottomOf="@+id/fundtransfer" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/deposit"
                android:layout_width="80dp"
                android:layout_height="77dp"
                android:layout_marginTop="84dp"
                android:background="@drawable/deposit"
                android:contentDescription="@string/desc_button"
                app:layout_constraintEnd_toEndOf="@+id/ImpHintsRecyclerview"
                app:layout_constraintHorizontal_bias="0.038"
                app:layout_constraintStart_toStartOf="@+id/ImpHintsRecyclerview"
                app:layout_constraintTop_toBottomOf="@+id/myprofile" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:text="Deposit"
                android:textColor="#FF000000"
                android:textSize="15sp"
                app:layout_constraintEnd_toEndOf="@+id/deposit"
                app:layout_constraintHorizontal_bias="0.541"
                app:layout_constraintStart_toStartOf="@+id/deposit"
                app:layout_constraintTop_toBottomOf="@+id/deposit" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/card_service"
                android:layout_width="80dp"
                android:layout_height="77dp"
                android:layout_marginTop="84dp"
                android:background="@drawable/card1"
                android:contentDescription="@string/desc_button"
                app:layout_constraintEnd_toEndOf="@+id/ImpHintsRecyclerview"
                app:layout_constraintHorizontal_bias="0.269"
                app:layout_constraintStart_toEndOf="@+id/deposit"
                app:layout_constraintTop_toBottomOf="@+id/mypassbook" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:text="Card Services"
                android:textColor="#FF000000"
                android:textSize="15sp"
                app:layout_constraintEnd_toEndOf="@+id/card_service"
                app:layout_constraintHorizontal_bias="0.541"
                app:layout_constraintStart_toStartOf="@+id/card_service"
                app:layout_constraintTop_toBottomOf="@+id/card_service" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/Service_request"
                android:layout_width="80dp"
                android:layout_height="77dp"
                android:layout_marginTop="84dp"
                android:background="@drawable/service1"
                android:contentDescription="@string/desc_button"
                app:layout_constraintEnd_toEndOf="@+id/ImpHintsRecyclerview"
                app:layout_constraintHorizontal_bias="0.795"
                app:layout_constraintStart_toEndOf="@+id/card_service"
                app:layout_constraintTop_toBottomOf="@+id/fundtransfer" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:text="Service Request"
                android:textColor="#FF000000"
                android:textSize="15sp"
                app:layout_constraintEnd_toEndOf="@+id/Service_request"
                app:layout_constraintHorizontal_bias="0.518"
                app:layout_constraintStart_toStartOf="@+id/Service_request"
                app:layout_constraintTop_toBottomOf="@+id/Service_request" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/govt"
                android:layout_width="80dp"
                android:layout_height="77dp"
                android:layout_marginTop="84dp"
                android:background="@drawable/newinsurance"
                android:contentDescription="@string/desc_button"
                app:layout_constraintEnd_toEndOf="@+id/deposit"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/deposit"
                app:layout_constraintTop_toBottomOf="@+id/deposit" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="101dp"
                android:layout_height="52dp"
                android:layout_marginTop="16dp"
                android:gravity="center"
                android:text="Govt Micro Insurance Scheme"
                android:textColor="#FF000000"
                app:layout_constraintEnd_toEndOf="@+id/govt"
                app:layout_constraintHorizontal_bias="0.21"
                app:layout_constraintStart_toStartOf="@+id/govt"
                app:layout_constraintTop_toBottomOf="@+id/govt" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/offers"
                android:layout_width="80dp"
                android:layout_height="77dp"
                android:layout_marginTop="88dp"
                android:background="@drawable/offers"
                android:contentDescription="@string/desc_button"
                app:layout_constraintEnd_toEndOf="@+id/card_service"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/card_service"
                app:layout_constraintTop_toBottomOf="@+id/card_service" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="69dp"
                android:layout_height="28dp"
                android:layout_marginTop="16dp"
                android:text="   Offers"
                android:textColor="#FF000000"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="@+id/offers"
                app:layout_constraintHorizontal_bias="0.384"
                app:layout_constraintStart_toStartOf="@+id/offers"
                app:layout_constraintTop_toBottomOf="@+id/offers" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/feedback"
                android:layout_width="80dp"
                android:layout_height="77dp"
                android:layout_marginTop="88dp"
                android:background="@drawable/feedback"
                android:contentDescription="@string/desc_button"
                app:layout_constraintEnd_toEndOf="@+id/Service_request"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/Service_request"
                app:layout_constraintTop_toBottomOf="@+id/Service_request" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="76dp"
                android:layout_height="22dp"
                android:layout_marginTop="20dp"
                android:text="Feedback"
                android:textColor="#FF000000"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="@+id/feedback"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@+id/feedback"
                app:layout_constraintTop_toBottomOf="@+id/feedback" />

        </androidx.constraintlayout.widget.ConstraintLayout>


    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>
